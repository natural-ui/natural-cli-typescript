,EAAEtB,IAAI,GAAGA,IAAI,CAACuB,OAAR,GAAkB,EADV;AAE1BC,IAAAA,KAAK,EAAE,CAAC;AAAEC,MAAAA,QAAQ,EAAE;AAAZ,KAAD;AAFmB,GAAZ,CAAjB,CAGE,iCAHF,CADL,CADJ,EAOKhB,SAAS,IACN,oBAAC,QAAD,eAAcnB,cAAd;AAA8B,IAAA,KAAK,EAAC;AAApC,MACK+B,iBAAiB,CAAC,UAAD,EAAa;AAC3BG,IAAAA,KAAK,EAAE,CAAC;AAAEC,MAAAA,QAAQ,EAAE;AAAZ,KAAD;AADoB,GAAb,CAAjB,CAEE,iCAFF,CADL,CARR,EAcI,oBAAC,QAAD,eAAcnC,cAAd;AAA8B,IAAA,KAAK,EAAC;AAApC,MACK+B,iBAAiB,CAAC,UAAD,EAAa;AAC3BC,IAAAA,YAAY,EAAEtB,IAAI,GAAGA,IAAI,CAAC0B,QAAR,GAAmB9B,YAAY,CAAC,CAAD,CADtB;AAE3B4B,IAAAA,KAAK,EAAE,CAAC;AAAEC,MAAAA,QAAQ,EAAE;AAAZ,KAAD;AAFoB,GAAb,CAAjB,CAIG,mCACK7B,YAAY,CAAC+B,GAAb,CAAiB,UAAAC,CAAC;AAAA,WACf,4BAAQ,MAAR;AAAe,MAAA,GAAG,EAAEA,CAApB;AAAuB,MAAA,KAAK,EAAEA;AAA9B,OACKA,CADL,CADe;AAAA,GAAlB,CADL,CAJH,CADL,CAdJ,CADJ,CADJ;AAiCH;;eAEc,MAAKC,MAAL,GACX5C,MAAM,CACF,UAAC6C,KAAD,EAAgC;AAAA,yBACkCA,KAAK,CAACC,SADxC;AAAA,MACpB7B,UADoB,oBACpBA,UADoB;AAAA,MACRC,UADQ,oBACRA,UADQ;AAAA,MACIC,QADJ,oBACIA,QADJ;AAAA,MACcC,eADd,oBACcA,eADd;AAE5B,SAAO;AAAEH,IAAAA,UAAU,EAAVA,UAAF;AAAcC,IAAAA,UAAU,EAAVA,UAAd;AAA0BC,IAAAA,QAAQ,EAARA,QAA1B;AAAoCC,IAAAA,eAAe,EAAfA;AAApC,GAAP;AACH,CAJC,CAAN,CAKEnB,QAAQ,CAACW,SAAD,CALV,CADW,C;;AAAf;;;;;;;;;;0BAlGMT,Q;0BAEAE,c;0BAWAM,Y;0BAeGC,S","sourcesContent":["import * as React from 'react'\nimport { inject } from 'mobx-react'\nimport { observer, useComputed } from 'mobx-react-lite'\nimport { Modal, Form, Input, Select } from 'antd'\nimport { FormComponentProps } from 'antd/lib/form'\n\nconst FormItem = Form.Item\n\nconst formItemLayout = {\n    labelCol: {\n        xs: { span: 24 },\n        sm: { span: 5 }\n    },\n    wrapperCol: {\n        xs: { span: 24 },\n        sm: { span: 19 }\n    }\n}\n\nconst userCategory = ['user', 'admin']\n\ninterface IStoreProps {\n    createUser?: (user: IUserStore.IUser) => Promise<any>\n    modifyUser?: (user: IUserStore.IUser) => Promise<any>\n    getUsers?: () => Promise<any>\n    changePageIndex?: (pageIndex: number) => void\n}\n\ninterface IProps extends IStoreProps, FormComponentProps {\n    visible: boolean\n    onCancel: () => void\n    user?: IUserStore.IUser\n}\n\nfunction UserModal({ visible, onCancel, user, form, createUser, modifyUser, getUsers, changePageIndex }: IProps) {\n    const [loading, setLoading] = React.useState(false)\n\n    const typeIsAdd = useComputed(() => user === undefined, [user])\n    const title = useComputed(() => (typeIsAdd ? 'Add User' : 'Modify User'), [typeIsAdd])\n\n    function toggleLoading() {\n        setLoading(l => !l)\n    }\n\n    function submit(e?: React.FormEvent<any>) {\n        if (e) {\n            e.preventDefault()\n        }\n        form.validateFields(\n            async (err, values): Promise<any> => {\n                if (!err) {\n                    toggleLoading()\n                    try {\n                        if (typeIsAdd) {\n                            await createUser(values)\n                            changePageIndex(1)\n                        } else {\n                            await modifyUser({ ...values, _id: user._id })\n                            getUsers()\n                        }\n                        onCancel()\n                    } catch (err) {}\n                    toggleLoading()\n                }\n            }\n        )\n    }\n\n    const { getFieldDecorator } = form\n    return (\n        <Modal title={title} visible={visible} onOk={submit} onCancel={onCancel} okButtonProps={{ loading }}>\n            <Form onSubmit={submit}>\n                <FormItem {...formItemLayout} label=\"account\">\n                    {getFieldDecorator('account', {\n                        initialValue: user ? user.account : '',\n                        rules: [{ required: true }]\n                    })(<Input />)}\n                </FormItem>\n                {typeIsAdd && (\n                    <FormItem {...formItemLayout} label=\"password\">\n                        {getFieldDecorator('password', {\n                            rules: [{ required: true }]\n                        })(<Input />)}\n                    </FormItem>\n                )}\n                <FormItem {...formItemLayout} label=\"category\">\n                    {getFieldDecorator('category', {\n                        initialValue: user ? user.category : userCategory[0],\n                        rules: [{ required: true }]\n                    })(\n                        <Select>\n                            {userCategory.map(c => (\n                                <Select.Option key={c} value={c}>\n                                    {c}\n                                </Select.Option>\n                            ))}\n                        </Select>\n                    )}\n                </FormItem>\n            </Form>\n        </Modal>\n    )\n}\n\nexport default Form.create<IProps>()(\n    inject(\n        (store: IStore): IStoreProps => {\n            const { createUser, modifyUser, getUsers, changePageIndex } = store.userStore\n            return { createUser, modifyUser, getUsers, changePageIndex }\n        }\n    )(observer(UserModal))\n)\n"]}]}